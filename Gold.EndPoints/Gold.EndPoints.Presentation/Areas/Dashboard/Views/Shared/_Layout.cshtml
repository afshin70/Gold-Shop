    <script src="~/themes/default/plugins/sweetalert/dist/sweetalert.min.js"></script>
    <script type="text/javascript" src="~/themes/default/js/plugins/bootstrap-toastr/toastr.min.js"></script>

    <script src="~/themes/default/js/pages/BaseJs.js" asp-append-version="true"></script>
    <script src="~/themes/default/js/custom.js" asp-append-version="true"></script>
    <script src="~/themes/default/js/mainJS.js" asp-append-version="true"></script>

    <script type="text/javascript" src="~/themes/default/js/core/app.min.js"></script>
