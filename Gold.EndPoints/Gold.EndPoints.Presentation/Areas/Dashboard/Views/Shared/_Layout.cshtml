    <script src="~/themes/default/js/mainJS.js" asp-append-version="true"></script>

    <script type="text/javascript" src="~/themes/default/js/core/app.min.js"></script>
    <script type="text/javascript" src="~/themes/default/js/plugins/ui/ripple.min.js"></script>*@

</head>
<body class="sidebar-xs">
    <div class="navbar navbar-inverse header-highlight ">
        <div class="navbar-header">
            <a class="navbar-brand" href="/">
                <b class="CompanyNameId">Shiraz Gold Shop</b>
            </a>

            <ul class="nav navbar-nav visible-xs-block">
                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
                <li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
        </div>

        <div class="navbar-collapse collapse" id="navbar-mobile">
            <ul class="nav navbar-nav">
                <li><a class="sidebar-control sidebar-main-toggle hidden-xs"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>

            <p class="navbar-text">
				<span class="label bg-success">ONLINE</span>@*کاربر @User.GetUserTypeFaName()*@
            </p>

            <div class="navbar-right">
                <ul class="nav navbar-nav">
                    <li class="dropdown dropdown-user">
                        @await Html.PartialAsync("_UserMenu")
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="page-container">

        <!-- Page content -->
        <div class="page-content">

            <!-- Main sidebar -->
            <div class="sidebar sidebar-main ">
                <div class="sidebar-content">
                    <!-- Main navigation -->
                    <div class="sidebar-category sidebar-category-visible">
                        <!-- User menu -->
                        @await Html.PartialAsync("~/Areas/Dashboard/Views/Shared/_LoginPartial.cshtml")
                        <!-- /user menu -->
                        <div class="category-content no-padding">
                            @await Html.PartialAsync("~/Areas/Dashboard/Views/Shared/_SidebarPartial.cshtml")
                        </div>
                    </div>
                    <!-- /main navigation -->

                </div>
            </div>
            <!-- /main sidebar -->
            <!-- Main content -->
            <div class="content-wrapper">
                @RenderBody()
            </div>
        </div>
    </div>
    <!-- Footer -->
    <div class="navbar navbar-default navbar-fixed-bottom footer">
        <ul class="nav navbar-nav visible-xs-block">
            <li><a class="text-center collapsed" data-toggle="collapse" data-target="#footer"><i class="icon-circle-up2"></i></a></li>
        </ul>
        <div class="navbar-collapse collapse" id="footer">
            <div class="navbar-text">
                &copy; @DateTime.UtcNow.Year. @Captions.DesignAndSupport: <a href="http://AraPendar.com" class="navbar-link" target="_blank">@Captions.AraPendar</a>
            </div>
            <div class="navbar-right">
                <ul class="nav navbar-nav">
                    <li><a href="#">@Captions.SiteVersion </a></li>@*@SystemSetting.Version*@
                </ul>
            </div>
        </div>
    </div>
    @{
        ModalModel changePasswordModel = new()
            {
                IsShowCloseButtonInFooter = false,
                Id = "changePasswordModal",
                IsShowCloseIcon = true,
                Size = ModalSize.Small,
                Title = Captions.ChangePassword,
            };
    }
    <partial name="_Modal" model="changePasswordModel" />

    <!-- /footer -->
    @*Widget Javascript*@
